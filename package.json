{
  "name": "jsdoc-api",
  "author": "Lloyd Brookes <75pound@gmail.com>",
  "version": "5.0.2",
  "description": "A programmatic interface for jsdoc",
  "repository": "https://github.com/jsdoc2md/jsdoc-api.git",
  "license": "MIT",
  "keywords": [
    "jsdoc",
    "api",
    "programmatic",
    "interface",
    "javascript",
    "documentation"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "test": "test-runner test/explain.js test/explain-sync.js test/render-sync.js && test-runner test/caching.js",
    "docs": "jsdoc2md -t jsdoc2md/README.hbs index.js lib/*.js > README.md; echo",
    "cover": "nyc npm test && nyc report --reporter=text-lcov | coveralls"
  },
  "devDependencies": {
    "collect-json": "^1.0.8",
    "coveralls": "^3.0.6",
    "jsdoc-to-markdown": "^5.0.0",
    "nyc": "^14.1.1",
    "rimraf": "^3.0.0",
    "test-runner": "~0.6.0"
  },
  "dependencies": {
    "array-back": "^4.0.0",
    "cache-point": "^0.4.1",
    "collect-all": "^1.0.3",
    "file-set": "^2.0.1",
    "fs-then-native": "^2.0.0",
    "jsdoc": "^3.6.3",
    "object-to-spawn-args": "^1.1.1",
    "temp-path": "^1.0.0",
    "walk-back": "^3.0.1"
  },
  "standard": {
    "ignore": [
      "tmp",
      "test/fixture"
    ]
  },
  "files": [
    "index.js",
    "lib/*.js"
  ]
}
